  "main": "./server/server.js",
  "scripts": {
    "start": "nodemon --ignore chaincode/ ./server/server.js --exec babel-node --inspect",
    "prebuild": "npm install",
    "build": "npm-run-all build:webpack build:server",
    "build:webpack": "cross-env NODE_ENV=production&&babel-node ./tools/build",
    "build:server": "babel server -d bin ./webpack/webpack.config.prod.js",
    "serve": "cross-env NODE_ENV=production&&babel-node ./server/server",
    "server": "npm run serve",
    "dev": "babel-node ./server/server",
    "lint": "eslint .",
    "autofix": "eslint --fix .",
    "clean": "rimraf -rf ./dist ./bin"
  },
